{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.mangadex.org/:path*"
    },
    {
      "source": "/covers/:path*",
      "destination": "https://uploads.mangadex.org/covers/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}